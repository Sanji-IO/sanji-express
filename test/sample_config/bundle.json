{
  "name": "nexthop",
  "version": "1.0",
  "author": "ChinFu.Yang",
  "email": "ChinFu.Yang@moxa.com",
  "description": "This is a nexthop model.",
  "license": "MOXA",
  "main": "nexthop.py",
  "argument": "",
  "priority": 90,
  "retry": 3,
  "retryInterval": 500,
  "hook": [
    "ethernet",
    "wireless"
  ],
  "dependencies": {
    "ethernet": "~1.0",
    "web": "~1.0"
  },
  "repository": {
    "type": "git",
    "url": "your git url"
  },
  "resources": [
    {
      "methods": "get",
      "uri": "/network/cellular"
    },
    {
      "methods": "get",
      "uri": "/system/export/download",
      "downloadFile": {
        "path": "/home/zack/samba/mar2000/romfsdisk/home/machine.json",
        "name": "machine.json"
      }
    },
    {
      "methods": [
        "put",
        "get"
      ],
      "uri": "/network/cellular/:id",
      "uploadFile": {
        "allowedFiles": [
          "firmware.zip",
          "key.pub"
        ],
        "dest": "./uploads/"
      }
    }
  ]
}
